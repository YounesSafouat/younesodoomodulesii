<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- WooCommerce Product Sync Cron -->
        <record id="ir_cron_woocommerce_sync" model="ir.cron">
            <field name="name">WooCommerce: Sync Products</field>
            <field name="model_id" ref="model_woocommerce_product"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_products()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active">False</field>
        </record>
        
    </data>
</odoo>
